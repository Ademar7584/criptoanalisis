{
  "name": "signal",
  "plural": "signals",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "tipo": {
      "type": "Boolean",
      "required": true
    },
    "tipoInversion": {
      "type": "string",
      "required": false
    },
    "AnalisisFundamental": {
      "type": "string",
      "required": false
    },
    "AnalisisTecnico": {
      "type": "string",
      "required": false
    },
    "EstrategiaSalida": {
      "type": "string",
      "required": false
    },
    "FechaCreate": {
      "type": "date",
      "required": false,
      "default": "$now"
    },
    "estado": {
      "type": "Boolean"
    },
    "visible": {
      "type": "Boolean"
    }
  },
  "validations": [],
  "relations": {
    "usuario": {
      "type": "belongsTo",
      "model": "usuario",
      "foreignKey": ""
    },
    "likes": {
      "type": "hasMany",
      "model": "usuario",
      "foreignKey": "SignalId",
      "through": "LikeSignal"
    },
    "puntos": {
      "type": "hasMany",
      "model": "position",
      "foreignKey": ""
    },
    "comments": {
      "type": "hasMany",
      "model": "usuario",
      "foreignkey": "signalId",
      "through": "commentSignal"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "update",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    }
  ],
  "methods": {},
  "mixins": {
    "Likes": [
        {
          "method": "like",
          "endpoint": "/:id/like",
          "likes": "likes",
          "userModel": "usuario"
        }
    ]
}
}
